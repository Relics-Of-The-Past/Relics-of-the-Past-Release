      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Grâce à ça, j'étais toujours premier à\nla course...\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Tu te rends compte, cela fait déjà 55 ans\nque j'en consomme..."
      - control:
          kind: pause
          length: longer
      - text: " Et maintenant,\nle "
      - control:
          kind: set_colour
