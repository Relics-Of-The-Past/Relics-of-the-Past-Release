  MSG_Futa_01:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Tu connais les monts Mère et Fille?\nIl paraît qu'on y trouve plein de\ngisements de minerai.\nJe n'ai plus qu'à trouver un marteau,\net à m'y remplir les poches.\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "À moi la fortune! "
      - control:
          kind: pause
          length: short
      - text: "Je vais être riiiiche!\n\n\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Hum... "
      - control:
          kind: pause
          length: long
      - text: "Excuse-moi...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "J'ai tendance à m'emballer quand je suis\nlancée sur le sujet..."
  MSG_Futa_01_1:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Il y a plein de gisements de minerai à\nexploiter sur les monts Mère et Fille,\nje n'ai plus qu'à me trouver un marteau!"
  MSG_Futa_02:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Si tu veux te rendre au "
      - control:
          kind: set_colour
          colour: blue
      - text: "village de\nCocorico"
      - control:
          kind: reset_colour
      - text: ", suis la route vers le nord.\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "J'y vais souvent pour vendre du minerai."
  MSG_Futa_02_1:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
