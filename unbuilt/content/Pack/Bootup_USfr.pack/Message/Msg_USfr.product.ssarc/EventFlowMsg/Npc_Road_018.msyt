  MSG_Futa_03:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: Hmm...
      - control:
          kind: pause
          length: long
      - text: "\n\n\nOh pardon!"
      - control:
          kind: pause
          length: short
      - text: " J'étais ailleurs...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Je pensais à ce que j'allais m'offrir en\npremier si je découvrais un gisement\nde "
      - control:
          kind: set_colour
          colour: blue
      - text: rubis
      - control:
          kind: reset_colour
      - text: " ou de "
      - control:
          kind: set_colour
          colour: blue
      - text: saphirs bruts
      - control:
          kind: reset_colour
      - text: ".\nCes deux gemmes valent très cher,\nil suffirait que j'en déniche une ou\ndeux,"
      - control:
          kind: pause
          length: short
      - text: " et à moi la vie de château!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: "Oups, pardon...\n\n\nQuand je pense à tout ce que je pourrais\nfaire avec quelques belles pierres... "
      - control:
          kind: pause
          length: long
      - text: "j'ai\ntendance à me laisser emporter."
  MSG_Futa_03_1:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Certaines gemmes comme les "
      - control:
          kind: set_colour
          colour: blue
      - text: saphirs
      - control:
          kind: reset_colour
      - text: "\nou les "
      - control:
          kind: set_colour
          colour: blue
      - text: rubis bruts
      - control:
          kind: reset_colour
      - text: " ont un prix tellement\nélevé que ça frise le ridicule.\nMais ça serait encore plus ridicule\nde ne pas en profiter, héhé."
  MSG_Futa_04:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "La plupart des gens apprécient les\ngemmes pour leur beauté, leur pureté,\nou d'autres considérations esthétiques.\nMais moi, c'est leur valeur\nque j'aime le plus!\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Il me suffit de penser à tout l'argent\nqu'elles peuvent rapporter pour avoir\ndes frissons..."
  MSG_Futa_04_1:
    attributes: Npc_Road_018
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Le plus important, c'est la beauté\nintérieure."
      - control:
          kind: pause
          length: long
      - text: " Et la beauté intérieure\nd'une gemme, c'est son prix."
