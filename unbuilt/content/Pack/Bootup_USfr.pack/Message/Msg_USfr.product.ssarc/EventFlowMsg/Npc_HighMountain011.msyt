          cancel_index: 1
          unknown: 6
  Talk_07:
    attributes: Npc_HighMountain011
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Cela dit, on s'est rendu compte qu'elle\nnous laissait tranquilles tant qu'on restait\nau sol.\nDu coup, pour entrer et sortir du village,\ntout le monde passe par cette porte,\nà pied.\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Franchement...
      - control:
          kind: pause
          length: short
      - text: " Si c'est pas malheureux."
  Talk_09:
    attributes: Npc_HighMountain011
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 19
      - text: Hein?
      - control:
          kind: pause
          length: short
