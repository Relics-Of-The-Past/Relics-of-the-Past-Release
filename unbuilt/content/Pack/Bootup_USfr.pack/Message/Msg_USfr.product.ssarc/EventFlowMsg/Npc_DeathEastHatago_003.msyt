            - 0
            - 0
      - text: "Si tu veux,"
      - control:
          kind: pause
          length: short
      - text: " je peux te vendre un\nde mes "
      - control:
          kind: set_colour
          colour: blue
      - text: remèdes ignifus
      - control:
          kind: reset_colour
      - text: "."
  talk33:
    attributes: Npc_DeathEastHatago_003
    contents:
      - control:
