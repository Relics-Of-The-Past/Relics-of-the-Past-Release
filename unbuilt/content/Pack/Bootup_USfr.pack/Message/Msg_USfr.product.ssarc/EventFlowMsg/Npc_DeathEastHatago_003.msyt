    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Écoute, je sais que nous ne nous\nsommes jamais vus,"
      - control:
          kind: pause
          length: short
      - text: " mais j'ai pas envie\nd'avoir ta mort sur la conscience...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: Ah...
      - control:
          kind: pause
          length: short
      - text: " Je vous jure...\n\n\nSi au moins tu avais des "
      - control:
          kind: set_colour
          colour: blue
      - text: remèdes ignifus
      - control:
          kind: reset_colour
      - text: ",\ntu pourrais résister à la chaleur."
      - control:
          kind: pause
          length: short
      - text: " Je peux\nt'en vendre si tu veux."
  talk31:
    attributes: Npc_DeathEastHatago_003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Si tu veux m'en acheter d'autres,\nn'hésite pas à revenir me voir."
  talk32:
    attributes: Npc_DeathEastHatago_003
    contents:
      - control:
          kind: sound
