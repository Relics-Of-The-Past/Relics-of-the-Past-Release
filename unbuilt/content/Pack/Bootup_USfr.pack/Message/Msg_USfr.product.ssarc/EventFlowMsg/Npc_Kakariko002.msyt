    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Ce...
      - control:
          kind: pause
          length: short
      - text: " Cet "
      - control:
          kind: set_colour
          colour: blue
      - text: orbe
      - control:
