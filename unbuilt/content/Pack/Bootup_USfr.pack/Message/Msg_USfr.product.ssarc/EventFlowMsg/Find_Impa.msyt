    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Encore pardon pour notre méfiance\nignorante à ton égard, la première fois...\n\nIl y a du mouvement du côté du "
      - control:
          kind: set_colour
          colour: blue
      - text: "clan\ndes Yigas"
      - control:
          kind: reset_colour
      - text: ", ces temps-ci..."
      - control:
          kind: pause
          length: short
      - text: " mais la\nprudence n'excuse pas l'ignorance."
      - control:
          kind: choice
          choice_labels:
            - 6
            - 7
