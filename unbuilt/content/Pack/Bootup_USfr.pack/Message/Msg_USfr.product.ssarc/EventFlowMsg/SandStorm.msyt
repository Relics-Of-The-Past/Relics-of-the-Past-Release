    attributes: Npc_SmallOasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: "... Mmh? "
      - control:
          kind: pause
          length: long
      - text: "Nooon... "
      - control:
          kind: pause
          length: short
      - text: "Si? "
      - control:
          kind: pause
          length: short
      - text: "La tempête se\nserait arrêtée?!\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Après toutes les heures que j'ai passées\nà la regarder... "
      - control:
          kind: pause
          length: long
      - text: "Pourquoi elle a disparu\ncomme ça? "
      - control:
          kind: pause
          length: short
      - text: Mystère et boule de rêve...
