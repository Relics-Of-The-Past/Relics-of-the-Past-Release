  Talk_000:
    attributes: Npc_oasis009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Un Hylien...? Tu te trouves à la "
      - control:
          kind: set_colour
          colour: blue
      - text: "porte\nnord-ouest"
      - control:
          kind: reset_colour
      - text: " de la cité Gerudo.\n\nLes voïs n'ont pas le droit d'entrer.\nC'est la règle."
