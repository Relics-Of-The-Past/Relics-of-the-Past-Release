  talk00:
    attributes: Npc_HighMountain004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Tiens, tiens, un visiteur!\n\n\n...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Mais...!\n\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Cette chose que tu portes à\nla ceinture, serait-ce...\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Mais où sont mes manières!
      - control:
          kind: pause
          length: longer
      - text: " Excuse-moi,\njeune homme. Je m'appelle Kaï, je suis\nle chef du village Piaf."
  talk01:
    attributes: Npc_HighMountain004
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 11
      - text: "Nom d'un petit oiseau!"
      - control:
          kind: pause
          length: short
      - text: "\nMais alors...\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Tu es donc un "
      - control:
          kind: set_colour
          colour: blue
      - text: Prodige
      - control:
          kind: reset_colour
      - text: ", comme le grand\nRevali, l'un des rares à pouvoir accéder\nà "
      - control:
          kind: set_colour
          colour: blue
      - text: "Vah'Medoh"
      - control:
          kind: reset_colour
      - text: "...\n"
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - control:
          kind: set_colour
          colour: grey
      - text: "Pourtant, c'est impossible... "
      - control:
          kind: pause
          length: short
      - text: "Les "
      - control:
          kind: set_colour
          colour: blue
      - text: "Prodiges "
      - control:
          kind: set_colour
          colour: grey
      - text: "ont tous\npéri il y a 100 ans..."
      - control:
          kind: pause
          length: longer
      - text: " Ce garçon doit être l'un de\nleurs descendants, héritier de la "
      - control:
          kind: set_colour
          colour: blue
      - text: tablette Sheikah
      - control:
          kind: set_colour
          colour: grey
      - text: "..."
      - control:
          kind: reset_colour
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - control:
          kind: text_size
          percent: 100
      - text: "Pardonne-moi, mon garçon!"
      - control:
          kind: pause
          length: longer
      - text: "\nJ'étais perdu dans mes pensées..."
  talk02:
    attributes: Npc_HighMountain004
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Ouh, ouh...\nAinsi donc, je faisais erreur."
  talk03:
    attributes: Npc_HighMountain004
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Ouh, ouh..."
  talk04:
    attributes: Npc_HighMountain004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Je te remercie infiniment.\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Maintenant que je sais que tu es\nl'héritier du "
      - control:
          kind: set_colour
          colour: blue
      - text: Prodige
      - control:
          kind: reset_colour
      - text: ","
      - control:
          kind: pause
          length: short
      - text: " j'ai une\nmission à te confier...\nVoyons, comment présenter la chose...\n\n\nC'est simple,"
      - control:
          kind: pause
          length: short
      - text: " j'aimerais que tu arrêtes\nla créature qui survole le village,"
      - control:
          kind: pause
          length: short
      - text: "\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Vah'Medoh"
      - control:
          kind: reset_colour
      - text: ".\nIl n'y a qu'un seul moyen d'arrêter une\n"
      - control:
          kind: set_colour
          colour: blue
      - text: créature divine
      - control:
          kind: reset_colour
      - text: " : un "
      - control:
          kind: set_colour
          colour: blue
      - text: "Prodige "
      - control:
          kind: reset_colour
      - text: "doit entrer\nà l'intérieur et en prendre le contrôle.\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Lorsque "
      - control:
          kind: set_colour
          colour: blue
      - text: Teba
      - control:
          kind: reset_colour
      - text: " et "
      - control:
          kind: set_colour
          colour: blue
      - text: Harfor
      - control:
          kind: reset_colour
      - text: ", deux guerriers\nde notre village, ont appris ça, leur sang\njeune et ardent n'a fait qu'un tour...\nIls se sont aussitôt élancés dans les\ncieux, bien déterminés à prendre le\ncontrôle de "
      - control:
          kind: set_colour
          colour: blue
      - text: Medoh
      - control:
          kind: reset_colour
      - text: "...\nMais la créature les a accueillis à coups\nde canon, et une de ses salves\nmeurtrières blessa le malheureux "
      - control:
          kind: set_colour
          colour: blue
      - text: Harfor
      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Héritier du "
      - control:
          kind: set_colour
          colour: blue
      - text: Prodige
      - control:
          kind: reset_colour
      - text: ","
      - control:
          kind: pause
          length: short
      - text: " je te supplie de\nnous aider.\n\nL'un des vaillants jeunes Piafs dont je te\nparlais, "
      - control:
          kind: set_colour
          colour: blue
      - text: Teba
      - control:
          kind: reset_colour
      - text: ", veut retenter sa chance\ncontre "
      - control:
          kind: set_colour
          colour: blue
      - text: Medoh
      - control:
          kind: reset_colour
      - text: ".\nTrouve-le. "
      - control:
          kind: pause
          length: short
      - text: "Si vous unissez vos forces,\nvous viendrez à bout de "
      - control:
          kind: set_colour
          colour: blue
      - text: "Vah'Medoh"
      - control:
          kind: reset_colour
      - text: "."
  talk05:
    attributes: Npc_HighMountain004
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: As-tu autre chose à me demander?
      - control:
