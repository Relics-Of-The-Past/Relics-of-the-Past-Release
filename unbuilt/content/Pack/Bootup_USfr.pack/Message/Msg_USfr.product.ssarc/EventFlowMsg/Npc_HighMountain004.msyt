  talk06:
    attributes: Npc_HighMountain004
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "As-tu aperçu l'"
      - control:
          kind: set_colour
          colour: blue
      - text: oiseau géant
      - control:
          kind: reset_colour
      - text: " qui plane\nau-dessus de notre village?\n\nIl s'agit de "
      - control:
          kind: set_colour
          colour: blue
      - text: "Vah'Medoh"
      - control:
          kind: reset_colour
      - text: ", une créature\ndivine.\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Les créatures divines nous viennent de\nl'ancien clan Sheikah."
      - control:
          kind: pause
          length: short
      - text: " Elles ont été\ncréées pour détruire "
      - control:
          kind: set_colour
          colour: red
      - text: le Fléau Ganon
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: reset_colour
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Mais il y a 100 ans, ces créatures divines\nont été vaincues par le monstre qu'elles\nétaient censées anéantir.\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Et aujourd'hui, la créature divine est\nde retour et s'attaque aveuglément aux\ninnocents...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Se pourrait-il que le Fléau Ganon soit\nde retour?"
  talk07:
    attributes: Npc_HighMountain004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Si tu veux nous aider, il te faut d'abord\nquelques informations, j'imagine..."
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
            - 6
            - 7
          selected_index: 0
          cancel_index: 3
          unknown: 10
  talk09:
    attributes: Npc_HighMountain004
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Ce dispositif que tu portes à la ceinture,\nc'est la "
      - control:
          kind: set_colour
          colour: blue
      - text: tablette Sheikah
      - control:
          kind: reset_colour
      - text: ", n'est-ce pas?"
      - control:
