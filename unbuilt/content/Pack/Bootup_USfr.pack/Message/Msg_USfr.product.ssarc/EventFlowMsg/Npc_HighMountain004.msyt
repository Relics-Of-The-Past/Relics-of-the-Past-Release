      - text: Sois mille fois remercié.
  talk26:
    attributes: Npc_HighMountain004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Héritier du "
      - control:
          kind: set_colour
          colour: blue
