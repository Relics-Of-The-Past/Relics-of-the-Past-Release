    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Je n'en attendais pas moins\nde l'héritier du Prodige.\n\nEt tu es le portrait craché du "
      - control:
          kind: set_colour
          colour: blue
      - text: "Prodige\nHylien"
      - control:
          kind: reset_colour
      - text: " qui s'est battu aux côtés du\nGrand Revali il y a 100 ans...\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Il ne te manque que la "
      - control:
          kind: set_colour
