  Npc_oasis037_SD_303:
    attributes: Npc_oasis037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Mais attends...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 8
      - text: "J'ai tout vu! J'arrive pas à croire que\nt'aies réussi à régler son compte à ce\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Moldarquor
      - control:
          kind: reset_colour
      - text: "!\nC'était dingue, je suis encore\nsous le choc!\n\nDis donc... tu veux bien que je\nrédige un article sur ton exploit?"
  Npc_oasis037_SD_400:
    attributes: Npc_oasis037
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "Dis donc, tu serais pas le garçon que j'ai\nrencontré dans la "
      - control:
          kind: set_colour
          colour: blue
      - text: "zone est de la région\nGerudo"
      - control:
          kind: reset_colour
      - text: "?\nMais la cité est interdite aux hommes !"
      - control:
          kind: pause
          length: long
      - text: "\nHmm."
      - control:
          kind: pause
          length: long
      - text: " À voir ton accoutrement, quelque\nchose me dit que t'es déjà au courant...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "C'est bon, c'est bon, je ne dirai rien!\nTu me prends pour qui, la commère de\nservice? Franchement!"
