  ChottoMatte00:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 4
      - text: "Toute seule depuis toutes ces années,\nla princesse Zelda se bat contre le\n"
      - control:
