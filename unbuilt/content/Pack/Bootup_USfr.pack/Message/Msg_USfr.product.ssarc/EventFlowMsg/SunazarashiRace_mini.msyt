          length: longer
  Npc_oasis039_R_002:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Bonne réponse, comme toujours!\nÀ moi les "
      - control:
          kind: set_colour
          colour: red
      - text: 100 rubis
      - control:
          kind: reset_colour
      - text: "!"
  Npc_oasis039_R_003:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Attends un peu! Ne te fiche pas de moi!\n\n\nTu n'as pas de "
      - control:
          kind: set_colour
          colour: red
      - text: bouclier
      - control:
          kind: reset_colour
      - text: "!\n\n\nTu n'as quand même pas oublié qu'il\nen fallait un pour conduire un "
      - control:
          kind: set_colour
          colour: blue
      - text: morse
      - control:
          kind: reset_colour
      - text: "?\nOu alors tu ne mérites pas ton titre!\nReviens quand tu auras "
      - control:
          kind: set_colour
          colour: red
      - text: "équipé\nun bouclier"
      - control:
          kind: reset_colour
      - text: "!"
  Npc_oasis039_R_004:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Le but est d'atteindre l'arrivée le plus\nvite possible à dos de "
      - control:
          kind: set_colour
          colour: blue
      - text: morse des sables
      - control:
          kind: reset_colour
      - text: ",\ntout en ayant passé les arches.\nSi un concurrent "
      - control:
          kind: set_colour
          colour: red
      - text: tombe de son morse
      - control:
          kind: reset_colour
      - text: "\nou "
      - control:
          kind: set_colour
          colour: red
      - text: "s'éloigne trop de la zone de course"
      - control:
          kind: reset_colour
      - text: "...\nil est éliminé!\nLe record actuel est : "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 14
          name: SunazarashiRace_BestTime_L
      - text: " min "
      - control:
          kind: variable
          variable_kind: 14
          name: SunazarashiRace_BestTime_M
      - text: " s "
      - control:
          kind: variable
          variable_kind: 14
          name: SunazarashiRace_BestTime_S
      - text: " cs"
      - control:
          kind: reset_colour
      - text: ".\nSi tu arrives à le battre, la récompense\nde "
      - control:
          kind: set_colour
          colour: red
      - text: 300 rubis
      - control:
          kind: reset_colour
      - text: " est à toi!"
      - control:
          kind: pause
          length: longer
      - text: "\nT'as tout compris? Allez, assez palabré!\n\n\nFile-moi "
      - control:
          kind: set_colour
          colour: red
      - text: 100 rubis
      - control:
          kind: reset_colour
      - text: " et la course peut\ncommencer."
