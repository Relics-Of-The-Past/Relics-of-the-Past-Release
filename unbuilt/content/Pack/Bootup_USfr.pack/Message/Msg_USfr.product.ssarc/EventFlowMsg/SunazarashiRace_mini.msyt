  Npc_oasis039_S1_003:
    attributes: Npc_oasis039
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Mais qu'est-ce que tu fais?! Je t'ai\npourtant dit qu'il ne fallait pas "
      - control:
          kind: set_colour
          colour: red
      - text: "trop\nt'éloigner"
      - control:
          kind: reset_colour
      - text: "!\nOn arrête tout!"