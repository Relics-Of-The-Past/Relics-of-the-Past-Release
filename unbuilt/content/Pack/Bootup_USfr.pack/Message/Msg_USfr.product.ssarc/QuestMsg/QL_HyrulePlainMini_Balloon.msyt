entries:
  QL_HyrulePlainMini_Balloon_Desc:
    contents:
      - text: "Vous avez promis à Shamae, une enfant du\nrelais de la forêt, de lâcher des "
      - control:
          kind: set_colour
          colour: red
      - text: ballons
      - control:
          kind: reset_colour
      - text: " pour\nqu'ils s'envolent dans le ciel.\n\nVous devez tout d'abord trouver un objet qui\npeut faire office de "
      - control:
          kind: set_colour
          colour: red
      - text: ballon
      - control:
          kind: reset_colour
      - text: ", puis en attacher\nquelques exemplaires à un des tonneaux.\nÇa lui fera sûrement très plaisir!"
  QL_HyrulePlainMini_Balloon_Finish:
    contents:
      - text: "Vous avez tenu votre promesse envers Shamae\nen faisant s'envoler des "
      - control:
          kind: set_colour
          colour: red
      - text: ballons octo
      - control:
          kind: reset_colour
      - text: ".\n\nPour vous remercier, elle vous a donné\nun "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: "fragment d'étoile."
  QL_HyrulePlainMini_Balloon_Name:
    contents:
      - text: "Volent, volent les ballons!"