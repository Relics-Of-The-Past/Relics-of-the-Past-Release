      - control:
          kind: set_colour
          colour: blue
      - text: lac Faroria
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "\nJe me demande à quel point tout cela\nest vrai."
  talk03:
    attributes: Npc_FaronWoods003
