            - 0
            - 0
      - text: "Dans ce cas, tu verras qu'au nord d'ici\nse trouvent les "
      - control:
          kind: set_colour
          colour: blue
      - text: monts Mère et Fille
      - control:
          kind: reset_colour
      - text: ".\n\nD'après la légende,"
      - control:
          kind: pause
          length: short
