          length: short
      - text: " Elle est drôle, ton aile. Je n'en\navais encore jamais vu de comme ça.\n\nTu aimerais suivre mon "
      - control:
          kind: set_colour
          colour: red
      - text: "entraînement\npour ne faire qu'un avec le vent"
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: choice
          choice_labels:
            - 5
            - 6
            - 7
          selected_index: 0
          cancel_index: 2
          unknown: 8
