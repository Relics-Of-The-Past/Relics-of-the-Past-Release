      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Oh!
      - control:
          kind: pause
          length: short
      - text: " Tu cherches quelqu'un pour\nouvrir une boutique là-bas?\n\n"
      - control:
          kind: sound
