  Com_Talk_00:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: Que souhaites-tu me vendre?
  Com_Talk_01:
    attributes: ""
    contents:
      - text: "Je t'en offre "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 18
          name: ShopTradePrice
      - text: " rubis"
      - control:
          kind: reset_colour
      - text: ".\nOn fait affaire?"
      - control:
          kind: choice
          choice_labels:
            - 5500
            - 5501
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Com_Talk_02:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: Merci bien.
      - control:
          kind: pause
          length: short
      - text: " Tu veux me vendre autre\nchose?"
  Com_Talk_02b:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: Merci!
  Com_Talk_02c:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Je n'aurai qu'un mot : merci!"
  Com_Talk_02d:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: Merci pour tout!
  Com_Talk_04:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: "Oh, c'est dommage."
  Com_Talk_05:
    attributes: Npc_Road_015
    contents:
      - text: "J'achète tout ce que tu vends."
  Com_Talk_05b:
    attributes: ""
    contents:
      - text: "Quand quelque chose est à vendre,\nje l'achète! Les affaires, faut que ça\ntourne et les rubis, faut que ça circule!"
  Com_Talk_05c:
    attributes: ""
    contents:
      - text: Tu veux me vendre quelque chose?
  Com_Talk_05d:
    attributes: ""
    contents:
      - text: "Dis-moi ce que tu veux me vendre,\nje te dirai combien je te l'achète!"
  Com_Talk_06:
    attributes: Npc_Road_015
    contents:
      - text: "Qu'est-ce qui te ferait plaisir?"
  Com_Talk_07:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: "Désolé... "
      - control:
          kind: pause
          length: short
      - text: "Mon stock a été dévalisé,\net cet article est épuisé."
  Com_Talk_08:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound2
          unknown:
            - 14
            - 205
      - text: "Quand on a pas de "
      - control:
          kind: set_colour
          colour: red
      - text: rubis
      - control:
          kind: reset_colour
      - text: ", on vend,\non achète pas. "
      - control:
          kind: pause
          length: short
      - text: "C'est la règle numéro 1\ndu commerce."
  Com_Talk_09:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound2
          unknown:
            - 14
            - 205
      - text: "Ta sacoche est pleine à ras bord, hélas.\nAlors, vends, jette ou casse, mais fais de\nla place."
  Com_Talk_10:
    attributes: ""
    contents:
      - control:
          kind: raw
          two_hundred_one:
            one_field:
              - 3
              - field_1: 0
      - control:
          kind: raw
          two_hundred_one:
            one_field:
              - 2
              - field_1: 0
      - control:
          kind: raw
          two_hundred_one:
            one_field:
              - 4
              - field_1: 0
      - control:
          kind: variable
          variable_kind: 11
          name: Shop_SelectItemName
      - text: "?"
      - control:
          kind: choice
          choice_labels:
            - 5502
            - 5503
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Com_Talk_11:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "Merci, merci!"
  Com_Talk_11b:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: Merci mille fois!
  Com_Talk_11c:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Eh bien, voilà une affaire rondement\nmenée. Merci, cher client!"
  Com_Talk_11d:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: Merci encore!
  Com_Talk_12:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: "Tu admires le poisson le plus exquis et\nles fruits les plus délectables du "
      - control:
          kind: set_colour
          colour: blue
      - text: "village\nd'Écaraille"
      - control:
          kind: reset_colour
      - text: ". "
      - control:
          kind: pause
          length: long
      - text: Je ne vends que le meilleur!
  Com_Talk_13:
    attributes: ""
    contents:
      - text: "Volontiers. Combien m'en vends-tu?"
      - control:
          kind: choice
          choice_labels:
            - 5500
            - 5508
            - 5503
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Com_Talk_14:
    attributes: ""
    contents:
      - control:
          kind: variable
          variable_kind: 15
          name: Shop_TradeItemNum
      - text: "... "
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: pause
          length: long
      - control:
          kind: text_size
          percent: 100
      - text: "Accepterais-tu de t'en séparer\npour "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 18
          name: ShopTradePrice
      - text: " rubis"
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: choice
          choice_labels:
            - 5500
            - 5501
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Com_Talk_15:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 14
            - 205
      - text: "Je regrette, mais je ne peux pas\ndécemment te priver de cela,\nce ne serait pas déontologique."
  Com_Talk_16:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 14
            - 205
      - text: "Je veux bien t'acheter tes vêtements,\nmais je préférerais que tu te changes\navant. Tu veux attraper la mort, ou quoi?"
  Com_Talk_20:
    attributes: ""
    contents:
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: "Tu arrives pile pour mon offre spéciale\n« temps de cochon », petit chanceux!"
      - control:
          kind: pause
          length: short
      - text: "\nAchète mon précieux "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
      - text: turbo max
      - control:
          kind: reset_colour
      - text: "!"
  Com_Talk_30:
    attributes: ""
    contents:
      - text: "Il y en a "
      - control:
          kind: variable
          variable_kind: 15
          name: Shop_TradeItemNum
      - text: "... Ce qui nous fait un total\nde "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 18
          name: ShopTradePrice
      - text: " rubis"
      - control:
          kind: reset_colour
      - text: ". Ça te va?"
      - control:
          kind: choice
          choice_labels:
            - 5502
            - 5503
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Com_Talk_31:
    attributes: ""
    contents:
      - text: "Bien sûr! Il t'en fallait combien?"
      - control:
          kind: choice
          choice_labels:
            - 5502
            - 5507
            - 5503
          selected_index: 0
          cancel_index: 2
          unknown: 8
  Com_Talk_33:
    attributes: ""
    contents:
      - text: "C'est tout?\nTrès bien, cela fera donc "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 18
          name: ShopTradePrice
      - text: " rubis"
      - control:
          kind: reset_colour
      - text: "\ntout rond. Qu'en dis-tu?"
      - control:
          kind: choice
          choice_labels:
            - 5500
            - 5501
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Com_Talk_34:
    attributes: Npc_Kakariko010
    contents:
      - text: Tout le stock?
      - control:
          kind: pause
          length: short
      - text: "\nIl y en a "
      - control:
          kind: variable
          variable_kind: 15
          name: Shop_TradeItemNum
      - text: ", ce qui fait grimper la\nfacture à "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 18
          name: ShopTradePrice
      - text: " rubis"
      - control:
          kind: reset_colour
      - text: ". OK?"
      - control:
          kind: choice
          choice_labels:
            - 5500
            - 5501
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Com_Talk_35:
    attributes: Npc_Kakariko010
    contents:
      - text: "Tu m'en vends "
      - control:
          kind: variable
          variable_kind: 15
          name: Shop_TradeItemNum
      - text: "?\nJe t'offre "
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 18
          name: ShopTradePrice
      - text: " rubis "
      - control:
          kind: reset_colour
      - text: "pour le lot.\nOn est d'accord?"
      - control:
          kind: choice
          choice_labels:
            - 5500
            - 5501
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Com_Talk_36:
    attributes: Npc_Kakariko010
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Et voilà, tu as dévalisé tout mon stock!\nAh, si tous les clients étaient comme\ntoi..."
  Com_Talk_37:
    attributes: Npc_Kakariko010
    contents:
      - control:
          kind: sound2
          unknown:
            - 14
            - 205
      - text: "Je n'ai plus rien à vendre! Merci d'être\nun aussi bon client! Tu veux voir ce que\nje propose d'habitude?"
      - control:
          kind: choice
          choice_labels:
            - 5500
            - 5501
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Com_Talk_56:
    attributes: ""
    contents:
      - text: "C'est tout, tu es sûr?\nSoit. Dans ce cas, je t'en demanderai\n"
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 18
          name: ShopTradePrice
      - text: " rubis"
      - control:
          kind: reset_colour
      - text: ". Tu signes?"
      - control:
          kind: choice
          choice_labels:
            - 5502
            - 5503
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Failed00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 18
      - text: "Pfiou... J'ai cru que ma dernière heure\navait sonné..."
  Failed01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 44
      - text: "Argh... J'en ai pris pour mon grade..."
  GoodBye00:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: À la prochaine!
  Guardian00:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 42
      - text: "Un "
      - control:
          kind: set_colour
          colour: blue
      - text: Gardien
      - control:
          kind: reset_colour
      - text: "! Il n'y a qu'une seule\nchose à faire dans ces cas-là : fuir\nà toutes jambes!"
  MSG_Firo_00:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Par ici, chers clients gourmands!\nHumez-moi le fumet appétissant des\nspécialités du "
      - control:
          kind: set_colour
          colour: blue
      - text: "village d'Écaraille"
      - control:
          kind: reset_colour
      - text: "!\nJe vends aux petits et aux grands, aux\ndames et aux seigneurs! À tous, à part\naux monstres ; eux, peuvent sécher!"
  MSG_Firo_00_1:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Par ici, chers clients avisés!"
      - control:
          kind: pause
          length: short
      - text: "\nVenez admirer les merveilleuses\nspécialités du "
      - control:
          kind: set_colour
          colour: blue
      - text: "village d'Écaraille"
      - control:
          kind: reset_colour
      - text: "!"
  MSG_Firo_01:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "À force de trimballer les spécialités du\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "village d'Écaraille"
      - control:
          kind: reset_colour
      - text: " aux quatre coins du\npays, je le connais comme ma sacoche!\nPar exemple : à l'ouest se trouve le "
      - control:
          kind: set_colour
          colour: blue
      - text: "lac\nHylia"
      - control:
          kind: reset_colour
      - text: "... et aussi une forêt qu'on appelle\nla "
      - control:
          kind: set_colour
          colour: blue
      - text: forêt de Firas
      - control:
          kind: reset_colour
      - text: ".\nMais ça suffira pour aujourd'hui, il n'y\na pas écrit « guide touristique » là-haut!"
  MSG_Firo_01_1:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "À l'ouest se trouve le "
      - control:
          kind: set_colour
          colour: blue
      - text: lac Hylia
      - control:
          kind: reset_colour
      - text: ", ainsi que\nla "
      - control:
          kind: set_colour
          colour: blue
      - text: forêt de Firas
      - control:
          kind: reset_colour
      - text: ". Voilà pour la jasette,\nplace aux affaires, maintenant..."
  MSG_Firo_02:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Les succulentes spécialités que je vends\npour trois fois rien ne sont pas les seuls\nattraits du "
      - control:
          kind: set_colour
          colour: blue
      - text: "village d'Écaraille"
      - control:
          kind: reset_colour
      - text: "...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "On y perpétue également la tradition du\n« jeu des coffres »... C'est à la fois\nludique et nostalgique!\nMais je ne t'en dis pas plus, place aux\naffaires. Il y a un temps pour parler aux\ngens et un temps pour faire de l'argent!"
  MSG_Firo_02_1:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Le "
      - control:
          kind: set_colour
          colour: blue
      - text: "village d'Écaraille"
      - control:
          kind: reset_colour
      - text: " est un endroit si\ncharmant, la nourriture y est si délicieuse\net le « jeu des coffres » si passionnant!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Mais assez de la nostalgie,\ncausons affaires!"
  MSG_Firo_03:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Mes poissons sont exquis!
      - control:
          kind: pause
          length: short
      - text: "\nMes fruits sont trop bons!"
      - control:
          kind: pause
          length: short
      - text: "\nEt mes prix, ils sont ce qu'ils sont!\nVenez vous régaler les yeux et les\npapilles des spécialités du "
      - control:
          kind: set_colour
          colour: blue
      - text: "village\nd'Écaraille"
      - control:
          kind: reset_colour
      - text: "!"
  MSG_Firo_03_1:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Par ici!
      - control:
          kind: pause
          length: short
      - text: " Venez vous délecter des fruits\njuteux et des succulents poissons qui\nfont la célébrité du sud du pays!"
  MSG_Firo_04:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Inutile de te rendre au "
      - control:
          kind: set_colour
          colour: blue
      - text: "village d'Écaraille"
      - control:
          kind: reset_colour
      - text: "\npour y acheter leurs produits, puisque je\nte les apporte à prix sacrifié...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Par contre, on peut y jouer au meilleur\njeu de tous les temps : j'ai nommé\nl'incomparable « jeu des coffres »!\nSi tu ne connais pas, tu passes à côté\nde quelque chose, crois-moi!"
  MSG_Firo_04_1:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: "Sais-tu que le "
      - control:
          kind: set_colour
          colour: blue
      - text: "village d'Écaraille"
      - control:
          kind: reset_colour
      - text: " propose\nun jeu tout ce qu'il y a de plus palpitant :\nl'inénarrable « jeu des coffres »?\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Va faire une partie à l'occasion, tu ne le\nregretteras pas. En attendant, aurais-tu\nune petite faim, par hasard?"
  Near00:
    attributes: Npc_Road_015
    contents:
      - text: Poissons et fruits!
  Near01:
    attributes: Npc_Road_015
    contents:
      - text: "Par ici, chers clients!"
  Near02:
    attributes: Npc_Road_015
    contents:
      - text: "Jetez un coup d'œil!"
  Near03:
    attributes: Npc_Road_015
    contents:
      - text: "Fruits frais, fruits frais!"
  Near04:
    attributes: Npc_Road_015
    contents:
      - text: "Zut la pluie, vive les rubis!"
  Rest_00:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Les poissons et les fruits du "
      - control:
          kind: set_colour
          colour: blue
      - text: "village\nd'Écaraille"
      - control:
          kind: reset_colour
      - text: " sont de véritables merveilles.\nSi je ne les vendais pas, j'en mangerais."
  Rest_01:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Moi qui pensais qu'il ferait un soleil\nradieux, j'en suis pour mes frais. "
      - control:
          kind: pause
          length: short
      - text: "Autant\nfaire la sieste en attendant que ça passe."
  Rest_02:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 16
      - text: "Je me moque bien de la pluie, mais\nle mauvais temps fait déprimer mon\ncourageux petit âne..."
  RetMSG_Firo_00:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Je passe le plus clair de mon temps à\nvadrouiller à tous les vents pour vendre\nles spécialités du "
      - control:
          kind: set_colour
          colour: blue
      - text: "village d'Écaraille"
      - control:
          kind: reset_colour
      - text: "...\nAlors, je connais les coins dangereux, et\nle "
      - control:
          kind: set_colour
          colour: blue
      - text: "relais des montagnes "
      - control:
          kind: reset_colour
      - text: "en fait partie.\nDes monstres en veux-tu, en v'là!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Bon, ce n'est pas que je m'ennuie,\nmais ma marchandise ne va pas se\nvendre toute seule..."
  RetMSG_Firo_00_1:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Le coin du "
      - control:
          kind: set_colour
          colour: blue
      - text: "relais des montagnes "
      - control:
          kind: reset_colour
      - text: "grouille\nde monstres... Qui veut s'y balader a\nintérêt à presser le pas!\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Enfin, je parle, je parle, et pendant ce\ntemps, mes produits sont toujours sur\nle dos de mon âne!"
  RetMSG_Firo_01:
    attributes: Npc_Road_015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
