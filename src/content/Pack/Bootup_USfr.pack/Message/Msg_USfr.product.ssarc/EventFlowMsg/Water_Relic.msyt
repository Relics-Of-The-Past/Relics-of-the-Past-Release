  Npc_Zora003_R_032:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Link, je sais que les choses doivent être\ntrès dures pour toi en ce moment,"
      - control:
          kind: pause
          frames: 10
      - text: " mais\nj'aimerais que tu entendes ma requête."
  Npc_Zora003_R_033:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Bel exploit, Sidon!"
  Npc_Zora003_R_034:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
