      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Enfin, non. Surtout, ne m'appelle pas\nCaly."
      - control:
          kind: pause
          length: short
      - text: " Appelle-moi Maître.\n\n"
      - control:
          kind: text_size
          percent: 80
      - text: "Personne ne veut jamais m'appeler Maître...\n\n\n"
