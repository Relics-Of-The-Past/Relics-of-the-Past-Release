  talk00:
    attributes: Npc_SouthernVillage006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Salut voyageur.
      - control:
          kind: pause
          length: long
      - text: " Moi c'est Bacchyas\nl'pêcheur,"
      - control:
          kind: pause
          length: long
      - text: " même si j'pêche pas vraiment\ngrand-chose en c'moment...\nY'a des "
      - control:
          kind: set_colour
          colour: red
      - text: monstres
      - control:
          kind: reset_colour
      - text: " qui hantent tous les\nmeilleurs coins d'pêche du côté d'la\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "plage d'Araiso"
      - control:
          kind: reset_colour
      - text: "...\nQuand j'pense à tout l'bon poisson que\nj'ramenais d'là-bas, ça m'décoiffe..."
  talk01:
    attributes: Npc_SouthernVillage006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Alors, qu'est-ce que tu en dis?"
      - control:
          kind: pause
          length: short
      - text: " Tu veux\nbien aller débarrasser la "
      - control:
          kind: set_colour
          colour: blue
      - text: "plage d'Araiso"
      - control:
          kind: reset_colour
      - text: "\ndes "
      - control:
          kind: set_colour
          colour: red
      - text: monstres
      - control:
          kind: reset_colour
      - text: " qui y rôdent?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk02:
    attributes: Npc_SouthernVillage006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "J'te laisse gérer l'nettoyage d'la "
      - control:
          kind: set_colour
          colour: blue
      - text: "plage\nd'Araiso"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " J't'aurais bien accompagné,\nmais faut que j'pense à ma famille..."
  talk03:
    attributes: Npc_SouthernVillage006
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Hé, bravo!"
      - control:
          kind: pause
          length: short
      - text: " Y'a plus un "
      - control:
          kind: set_colour
          colour: red
      - text: monstre
      - control:
          kind: reset_colour
      - text: " debout\nsur la "
      - control:
          kind: set_colour
          colour: blue
      - text: "plage d'Araiso"
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: "\n\nOn peut donc repartir pêcher là-bas."
      - control:
          kind: pause
          length: short
      - text: " Et\nc'est tant mieux, parce que les poissons\nd'Araiso sont délicieux!"
      - control:
          kind: pause
          length: short
      - text: "\nPrends ça, c'est le moins que j'puisse\nfaire pour te remercier!"
  talk04:
    attributes: Npc_SouthernVillage006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Reviens quand tu veux!
      - control:
          kind: pause
          length: short
      - text: " J'te ferai\ngoûter l'meilleur poisson d'ta vie!"
  talk05:
    attributes: Npc_SouthernVillage006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 33
      - text: Mmm...
  talk06:
    attributes: Npc_SouthernVillage006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 34
      - text: "On va s'mettre au lit, là."
      - control:
          kind: pause
          length: short
      - text: "\nT'as juste à revenir demain."
  talk07:
    attributes: Npc_SouthernVillage006
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: Wahaha!
      - control:
          kind: pause
          length: short
      - text: " Voilà c'que j'aime entendre!\n\n\nSi tu vas à la "
      - control:
          kind: set_colour
          colour: blue
      - text: "plage d'Araiso"
      - control:
          kind: reset_colour
      - text: ", tu verras\nque les "
      - control:
          kind: set_colour
          colour: red
      - text: monstres
      - control:
          kind: reset_colour
      - text: " y ont monté un camp.\n\nJ'vais pas t'faire l'affront de t'expliquer\nc'que tu dois en faire..."
  talk08:
    attributes: Npc_SouthernVillage006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "C'est pas l'envie qui m'manque!"
      - control:
          kind: pause
          length: short
      - text: "\nMon harpon m'démange rien que\nd'y penser!\nMais j'dois penser à ma famille..."
      - control:
          kind: pause
          length: short
      - text: "\nJ'peux pas m'permettre qu'il\nm'arrive un truc pas bien..."
  talk09:
    attributes: Npc_SouthernVillage006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "J'te laisse faire le ménage à la "
      - control:
          kind: set_colour
          colour: blue
      - text: "plage\nd'Araiso"
      - control:
          kind: reset_colour
      - text: ".\n\nAvec un grain comme aujourd'hui,\nles "
      - control:
          kind: set_colour
          colour: red
      - text: monstres
      - control:
          kind: reset_colour
      - text: " s'ront moins sur leurs\ngardes que d'habitude."
  talk10:
    attributes: Npc_SouthernVillage006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Tu m'as l'air plutôt fort, dis-moi."
      - control:
          kind: pause
          length: short
      - text: "\nÇa t'tenterait pas d'aller leur botter\nles fesses?"
      - control:
