    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Tu m'épates, l'ami!"
      - control:
          kind: pause
          length: longer
      - text: " Comment tu as fait\npour remonter ce coffre?\n\n"
      - control:
          kind: sound
