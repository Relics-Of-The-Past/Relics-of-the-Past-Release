  NewText_00:
    attributes: Npc_NorthHatelHatago001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Ah, excuse-moi."
      - control:
          kind: pause
          length: short
      - text: " J'étais en pleine\nréflexion."
