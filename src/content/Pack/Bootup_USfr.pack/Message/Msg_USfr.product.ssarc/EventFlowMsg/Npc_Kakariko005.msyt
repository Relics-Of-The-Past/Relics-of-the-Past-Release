          unknown: 6
  Talk29:
    attributes: Npc_Kakariko005
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Ma femme m'a demandé de choisir..."
      - control:
          kind: pause
          length: long
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Elle m'a dit : « Vocah, ça suffit comme\nça! Tu dois choisir! C'est les cocottes\nou moi! »"
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Je n'ai pas su quoi répondre..."
      - control:
          kind: pause
          length: long
      - text: "\nAlors, elle est partie.\n\nEt pour couronner le tout,\nmes cocottes..."
      - control:
          kind: pause
          length: longer
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Non, je ne dois pas..."
      - control:
          kind: pause
          length: longer
      - text: " penser à ça.\nIl faut travailler.\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Mais voilà que maintenant,"
      - control:
          kind: pause
          length: short
      - text: " j'ai\nl'impression d'entendre mes cocottes..."
      - control:
