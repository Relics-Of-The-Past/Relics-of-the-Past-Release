  Suprise09:
    attributes: Npc_Kakariko005
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 21
      - text: Cherrola!
      - control:
          kind: pause
          length: long
      - text: " Tu es revenue!\n\n\n."
      - control:
          kind: pause
          length: short
      - text: "."
      - control:
          kind: pause
          length: short
      - text: "."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\nOh... C'est toi...\n\n\nPendant un instant, j'ai cru que c'était\nma femme qui revenait à la maison...\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Tant pis...
  Talk00:
    attributes: Npc_Kakariko005
    contents:
      - control:
          kind: sound
