      - text: "!"
      - control:
          kind: pause
          frames: 75
      - text: " Va "
      - control:
          kind: set_colour
          colour: red
      - text: suivre le chemin
      - control:
          kind: reset_colour
      - control:
          kind: pause
          length: long
      - text: "\n"
      - control:
          kind: set_colour
          colour: red
      - text: des anneaux du cratère
      - control:
          kind: reset_colour
      - text: "!"
  Npc_Musician_AoC_HeroGoronRelief_109:
    contents:
      - control:
          kind: auto_advance
          frames: 130
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1053609165
      - text: "Et "
      - control:
          kind: set_colour
          colour: red
      - text: de fouler la lave
      - control:
          kind: reset_colour
      - text: ","
      - control:
          kind: pause
          length: long
      - text: " "
      - control:
          kind: set_colour
          colour: red
      - text: "découvre la\nmanière"
      - control:
          kind: reset_colour
      - text: "!\n\n"
      - control:
          kind: auto_advance
          frames: 200
      - text: "Ton courage invincible,"
      - control:
          kind: pause
          length: long
      - text: " qui ici t'a mené,"
      - control:
          kind: pause
          length: long
      - text: "\npar la dernière épreuve"
      - control:
          kind: pause
          length: long
      - text: " se verra éprouvé."
  Npc_Musician_AoC_HeroGoronRelief_201:
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "C'était il y a cent ans, mais aujourd'hui\nencore,"
      - control:
          kind: pause
          length: long
      - text: " la bravoure du Prodige battrait\ntous les records!"
  Npc_Musician_AoC_HeroGoronRelief_202:
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 24
      - text: "Toujours mon maître a dit que pour\nécrire un chant,"
      - control:
          kind: pause
          length: long
      - text: " il fallait voyager et\nrencontrer les gens."
  Npc_Musician_AoC_HeroGoronRelief_203:
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ce principe, je l'ai mis en application,"
      - control:
          kind: pause
          length: long
      - text: " et\nje me suis rendu au "
      - control:
          kind: set_colour
          colour: blue
      - text: village des Gorons
      - control:
          kind: reset_colour
      - text: ".\n\nJ'ai appris des histoires par le jeune\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Yunobo
      - control:
          kind: reset_colour
      - text: ","
      - control:
          kind: pause
          length: long
      - text: " d'autres encore par son chef,\nle vigoureux "
      - control:
          kind: set_colour
          colour: blue
      - text: Buldo
      - control:
          kind: reset_colour
      - text: "."
  Npc_Musician_AoC_HeroGoronRelief_204:
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ces récits partagés m'ont touché jusqu'à\nl'âme!"
      - control:
          kind: pause
          length: long
      - text: " De mon "
      - control:
          kind: set_colour
          colour: blue
      - text: couplet manquant
      - control:
          kind: reset_colour
      - text: ", ils\nformeront la trame."