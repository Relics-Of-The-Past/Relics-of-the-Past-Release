group_count: 5
atr1_unknown: 0
entries:
  QL_CarryingBlueFireEXMini_Camera:
    contents: []
  QL_CarryingBlueFireEXMini_Carry:
    contents:
      - text: "Vous avez allumé le fourneau avec une flamme\nbleue et la pierre guide du laboratoire s'est\nremise à fonctionner.\n\nAllez "
      - control:
          kind: set_colour
          colour: red
      - text: prévenir
      - control:
          kind: reset_colour
      - text: " la "
      - control:
          kind: set_colour
          colour: red
      - text: chef du laboratoire
      - control:
          kind: reset_colour
      - text: " et faites\nréparer votre tablette Sheikah."
  QL_CarryingBlueFireEXMini_Desc:
    contents:
      - text: "D'après Impa, Zelda vous aurait laissé la\ntablette Sheikah pour qu'elle vous guide,\nmais elle serait incomplète...\n\nSuivez la carte et allez voir la "
      - control:
          kind: set_colour
          colour: red
      - text: chef
      - control:
          kind: reset_colour
      - text: " du\nlaboratoire antique d'Elimith. Cette spécialiste\nde la tablette Sheikah pourra sûrement\nvous aider."
  QL_CarryingBlueFireEXMini_Finish:
    contents:
      - text: "Vous avez dit à Impa que la tablette Sheikah\navait retrouvé ses modules de base!\n\nD'après elle, les photographies contenues dans\nl'album seraient des souvenirs laissés par Zelda\n100 ans auparavant.\n\nRetrouvez les endroits où elles ont été prises et\nvos propres souvenirs par la même occasion!"
  QL_CarryingBlueFireEXMini_Fired:
    contents:
      - text: "Grâce à Pru'ha, la tablette Sheikah a récupéré\nles "
      - control:
          kind: set_colour
          colour: red
      - text: modules
      - control:
          kind: reset_colour
      - text: " dont elle disposait il y a 100 ans.\n\nProfitez-en pour photographier Pru'ha avec\nun de ces "
      - control:
          kind: set_colour
          colour: red
      - text: modules
      - control:
          kind: reset_colour
      - text: " de base, l'"
      - control:
          kind: set_colour
          colour: red
      - text: appareil photo
      - control:
          kind: reset_colour
      - text: "!"
  QL_CarryingBlueFireEXMini_Name:
    contents:
      - text: "L'héritage de Zelda"
  QL_CarryingBlueFireEXMini_Permit:
    contents:
      - text: "La chef du laboratoire antique d'Elimith a\nbesoin de la pierre guide pour réparer la\ntablette Sheikah, mais elle est hors service.\nPour la démarrer, il suffirait d'allumer le\nfourneau attenant au laboratoire avec une\nflamme bleue. C'est à vous que revient la tâche\nd'en "
      - control:
          kind: set_colour
          colour: red
      - text: transporter une
      - control:
          kind: reset_colour
      - text: " du fourneau antique du\nvillage jusqu'au "
      - control:
          kind: set_colour
          colour: red
      - text: fourneau
      - control:
          kind: reset_colour
      - text: " du laboratoire."
  QL_CarryingBlueFireEXMini_Repaired:
    contents:
      - text: "Vous avez photographié Pru'ha avec\nl'appareil photo!\n\nD'après Pru'ha, l'album contenait déjà\nd'anciennes photos.\n\nAllez au village de Cocorico et demandez\nà "
      - control:
          kind: set_colour
          colour: red
      - text: Impa
      - control:
          kind: reset_colour
      - text: " si elle en sait plus sur ces photos."